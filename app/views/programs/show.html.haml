%h1 Program

.program
  = @program.name
  - @program.alternate_titles.each do |at|
    = at.name
  = @program.year
  = @program.version
  = @program.minutes
  - @program.directors.each do |dir|
    = dir.name
  - @program.series.each do |ser|
    = ser.name

.links
  =link_to "Programs", programs_url